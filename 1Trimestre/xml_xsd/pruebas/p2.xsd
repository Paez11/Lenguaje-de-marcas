<?xml version="1.0" encoding="UTF-8"?>

<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="urlset">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="url" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="loc" type="xs:anyURI"/>
                            <xs:element name="review" type="xs:positiveInteger"/>
                        </xs:sequence>
                        <xs:attribute name="lastMod" type="xs:date"/>
                        <xs:attribute name="lastmod" type="xs:date"/>
                        <xs:attribute name="priority" type="xs:decimal" use="required"/>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>